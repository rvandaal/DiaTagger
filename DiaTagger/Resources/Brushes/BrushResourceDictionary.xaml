                     Color="{StaticResource MedtronicGray03Color}" />
    <SolidColorBrush x:Key="TabItemBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGray01Color}" />
    <SolidColorBrush x:Key="TabItemBorderBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGray04Color}" />
    <SolidColorBrush x:Key="TabItemVertFlowBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGray01Color}" />

