        <GradientStop Offset="1" Color="{StaticResource MedtronicBlue05Color}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="MedtronicToggleSwitchMouseOverBackgroundBrush" po:Freeze="True" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Offset="0" Color="{StaticResource MedtronicBlue03Color}" />
        <GradientStop Offset="1" Color="{StaticResource MedtronicBlue04Color}" />
    </LinearGradientBrush>

    <!--  Blue04  -->
    <SolidColorBrush x:Key="MedtronicButtonCheckedBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicBlue04Color}" />
