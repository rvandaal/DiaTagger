    <SolidColorBrush x:Key="DataGridGridLineBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGray04Color}" />

    <!--  ButtonCustom transparent brushes  -->
    <SolidColorBrush x:Key="ButtonCustomTransparentActiveForegroundBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGray05Color}" />

