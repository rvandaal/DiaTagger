    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="ColorResourceDictionary.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <!--  To move  -->

    <SolidColorBrush x:Key="MedtronicInteractionBarBackgroundBrush" po:Freeze="True"
                     Color="#2B2B2B" />
    <SolidColorBrush x:Key="MedtronicInteractionBarBorderBrush" po:Freeze="True"
                     Color="#424242" />
    <SolidColorBrush x:Key="MedtronicInteractionControlBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicBlue07Color}" />
    <SolidColorBrush x:Key="MedtronicInteractionControlMouseOverBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicBlue08Color}" />
    <SolidColorBrush x:Key="MedtronicInteractionBarShadowBrush" po:Freeze="True"
                     Color="#151515" />

    <!--  Blue01  -->
    <SolidColorBrush x:Key="MedtronicListItemMouseOverBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicBlue01Color}" />

    <!--  Blue02  -->
