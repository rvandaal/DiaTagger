                     Color="{StaticResource MedtronicBlue05Color}" />
    <SolidColorBrush x:Key="MedtronicButtonPressedBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicBlue05Color}" />

    <SolidColorBrush x:Key="MedtronicAcceptedTaskIconBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicBlue05Color}" />
