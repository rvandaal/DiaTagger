                     Color="{StaticResource WhiteColor}" />
    <SolidColorBrush x:Key="TabItemDisabledBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGray03Color}" />
    <SolidColorBrush x:Key="TabItemDisabledBorderBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGray03Color}" />
