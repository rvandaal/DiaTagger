                     Color="{StaticResource MedtronicGray04Color}" />

    <SolidColorBrush x:Key="MedtronicLeadBodyBorderBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGray04Color}" />
    <SolidColorBrush x:Key="SidePanelExpanderChevronBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGray04Color}" />

    <!--  Gray05  -->

    <!--  Gray06  -->

