    <SolidColorBrush x:Key="DefaultTextBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGray04Color}" />
    <SolidColorBrush x:Key="ProgressBarInnerTextBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGray03Color}" />
    <SolidColorBrush x:Key="DefaultTextDarkBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGray01Color}" />

    <SolidColorBrush x:Key="ShadowBrush" po:Freeze="True"
                     Color="{StaticResource ShadowColor}" />
    <!--  Temporary hardcoded color value  -->
    <SolidColorBrush x:Key="TabItemShadowBrush" po:Freeze="True"
                     Color="#FF707070" />

    <SolidColorBrush x:Key="DefaultErrorTextBrush" po:Freeze="True"
                     Color="{StaticResource SapiensRed00Color}" />
