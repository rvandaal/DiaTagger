                     Color="{StaticResource MedtronicGray03Color}" />

    <LinearGradientBrush x:Key="MedtronicCancelButtonBackgroundBrush" po:Freeze="True" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Offset="0" Color="{StaticResource MedtronicGray02Color}" />
        <GradientStop Offset="1" Color="{StaticResource MedtronicGray03Color}" />
    </LinearGradientBrush>

