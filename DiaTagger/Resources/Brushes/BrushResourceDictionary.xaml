        <GradientStop Offset="0" Color="{StaticResource MedtronicRed01Color}" />
        <GradientStop Offset="1" Color="{StaticResource MedtronicRed03Color}" />
    </LinearGradientBrush>

    <!--  Red03  -->

    <!--  Red04  -->
    <SolidColorBrush x:Key="MedtronicNegativeButtonPressedBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicRed04Color}" />

    <!--  Green  -->
    <SolidColorBrush x:Key="MedtronicPositiveButtonMouseOverBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGreen01Color}" />
    <SolidColorBrush x:Key="MedtronicPositiveButtonPressedBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGreen02Color}" />

    <LinearGradientBrush x:Key="MedtronicPositiveButtonBackgroundBrush" po:Freeze="True" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Offset="0" Color="{StaticResource MedtronicGreen01Color}" />
        <GradientStop Offset="1" Color="{StaticResource MedtronicGreen02Color}" />
    </LinearGradientBrush>

    <!--  White  -->
    <SolidColorBrush x:Key="MedtronicBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource WhiteColor}" />
    <SolidColorBrush x:Key="MedtronicListItemSelectedForegroundBrush" po:Freeze="True"
                     Color="{StaticResource WhiteColor}" />
    <SolidColorBrush x:Key="MedtronicButtonCheckedForegroundBrush" po:Freeze="True"
                     Color="{StaticResource WhiteColor}" />
    <SolidColorBrush x:Key="MedtronicButtonActiveForegroundBrush" po:Freeze="True"
                     Color="{StaticResource WhiteColor}" />
    <SolidColorBrush x:Key="MedtronicButtonBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource WhiteColor}" />
    <SolidColorBrush x:Key="MedtronicInputBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource WhiteColor}" />
    <SolidColorBrush x:Key="MedtronicImportantButtonForegroundBrush" po:Freeze="True"
                     Color="{StaticResource WhiteColor}" />
    <SolidColorBrush x:Key="MedtronicPatientBarMajorTextBrush" po:Freeze="True"
                     Color="{StaticResource WhiteColor}" />

    <!--  =====================  -->
    <!--  OLD sapiens brushes  -->
    <!--  =====================  -->

    <!--  General brushes  -->
    <SolidColorBrush x:Key="MainBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGray02Color}" />
    <SolidColorBrush x:Key="GeneralBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource WhiteColor}" />
    <SolidColorBrush x:Key="GeneralForegroundBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGray04Color}" />

