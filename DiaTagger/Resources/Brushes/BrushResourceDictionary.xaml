                     Color="{StaticResource MedtronicGray04Color}" />

    <LinearGradientBrush x:Key="LeadBodyBackgroundBrush" po:Freeze="True" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Offset="0.5" Color="#99CCCC" />
        <GradientStop Offset="1" Color="#949494" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="LeadContactFloatingBackgroundBrush" po:Freeze="True" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Offset="0.5" Color="White" />
        <GradientStop Offset="1" Color="#F7F7F7" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="LeadContactAnodeBackgroundBrush" po:Freeze="True" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Offset="0.5" Color="#FF8833" />
        <GradientStop Offset="1" Color="#E6792E" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="LeadContactCathodeBackgroundBrush" po:Freeze="True" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Offset="0.5" Color="#0050FF" />
        <GradientStop Offset="1" Color="#295DCC" />
    </LinearGradientBrush>


    <!--  Gray01  -->

    <!--  Gray02  -->
    <SolidColorBrush x:Key="MedtronicCancelButtonMouseOverBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGray02Color}" />
