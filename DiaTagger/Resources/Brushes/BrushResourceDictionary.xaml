                     Color="{StaticResource MedtronicGray02Color}" />
    <SolidColorBrush x:Key="MedtronicProgressBarBackgroundBrush" po:Freeze="True"
                     Color="{StaticResource MedtronicGray02Color}" />
    <SolidColorBrush x:Key="MedtronicGridLineBrush" po:Freeze="True"
                     Opacity="0.3"
